{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "alkoumi/laravel-arabic-tafqeet": "dev-master",
        "arcanedev/log-viewer": "~8.1.0",
        "barryvdh/laravel-snappy": "^0.4.8",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "khaled.alshamaa/ar-php": "^6.2",
        "laravel/framework": "^8.0",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^8.0",
        "orangehill/iseed": "^3.0",
        "propaganistas/laravel-phone": "^4.3",
        "pusher/pusher-php-server": "^7.0",
        "realrashid/sweet-alert": "^3.2",
        "salla/zatca": "^1.0",
        "sentry/sentry-laravel": "^2.11",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-activitylog": "^4.3",
        "spatie/laravel-permission": "4.0.0",
        "spatie/laravel-translatable": "^4.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "uxweb/sweet-alert": "^2.0",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.18"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/envoy": "^2.7",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "tymon/jwt-auth": "^1.0.0-beta.3@dev"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "files": [
            "app/helper/helper.php",
            "app/helper/num_to_ar.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
